{
  "name": "mooniswap-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/CryptoManiacsZone/mooniswap-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/",
    "build": "graph build",
    "auth": "graph auth https://api.thegraph.com/deploy/ 6e069d9a5c8841e8904b0ec4424e5dad",
    "create": "graph create lombardi22/polygon --node https://api.thegraph.com/deploy/",
    "deploy": "graph deploy lombardi22/emiswap8 --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --debug",
    "deploy-polygon": "graph deploy lombardi22/polygon --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --debug",
    "deploy-avalanche": "graph deploy lombardi22/avalanche --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --debug",
    "create-kcc": "graph create emiswap/emiswap1 --node https://thegraph.kcc.network/deploy",
    "deploy-kcc": "graph deploy emiswap/emiswap1 --ipfs https://thegraph.kcc.network/ipfs/ --node https://thegraph.kcc.network/deploy/ --debug",
    "allin2": "graph codegen --output-dir src/types/ && graph build && graph deploy kiselevmaxim/emiswap4 --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --debug",
    "create-local": "graph create EMISWAP-COM/emiswap-subgraph --node http://localhost:8020",
    "deploy-local": "graph deploy EMISWAP-COM/emiswap-subgraph --ipfs http://localhost:5001 --node http://localhost:8020"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.16.0",
    "@graphprotocol/graph-ts": "^0.16.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  }
}
